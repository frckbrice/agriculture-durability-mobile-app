{
  "cli": {
    "version": ">= 3.0.0",
    "appVersionSource": "remote"
  },
  "build": { 
    "android": {
      "distribution": "store",
      "autoIncrement": true
    },
    "preview_debug": {
      "android": {
        "distribution": "internal",
        "gradleCommand": ":app:assembleDebug"
      },
      "ios": {
        "simulator": true,
        "buildConfiguration": "Debug"
      }
    },
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal",
      "env": {
        "GOOGLE_MAPS_API_KEY": "${GOOGLE_MAPS_API_KEY}",
        "EXPO_PUBLIC_PROD_API_URL": "${PROD_API_URL}",
        "EXPO_PUBLIC_CLERK_PUBLISHABLE_KEY": "${CLERK_PUBLISHABLE_KEY}",
        "EXPO_PUBLIC_AWS_ACCESS_KEY_ID": "${AWS_SECRET_KEY_ID}",
        "EXPO_PUBLIC_AWS_SECRET_ACCESS_KEY": "$(AWS_SECRET_KEY)",
        "EXPO_PUBLIC_AWS_REGION": "eu-west-1",
        "EXPO_PUBLIC_AWS_S3_BUCKET_NAME": "${AWS_BUCKET_NAME}",
        "JWT_SECRETE": "${JWT_SECRETE}",
        "JWT_TOKEN_EXPIRE_AFTER": "1d"
      }
    },
    "production": {
      "autoIncrement": true,
      "env": {
        "GOOGLE_MAPS_API_KEY": "${GOOGLE_MAPS_API_KEY}",
        "EXPO_PUBLIC_PROD_API_URL": "${PROD_API_URL}",
        "EXPO_PUBLIC_CLERK_PUBLISHABLE_KEY": "${CLERK_PUBLISHABLE_KEY}",
        "EXPO_PUBLIC_AWS_ACCESS_KEY_ID": "${AWS_SECRET_KEY_ID}",
        "EXPO_PUBLIC_AWS_SECRET_ACCESS_KEY": "$(AWS_SECRET_KEY)",
        "EXPO_PUBLIC_AWS_REGION": "eu-west-1",
        "EXPO_PUBLIC_AWS_S3_BUCKET_NAME": "${AWS_BUCKET_NAME}",
        "JWT_SECRETE": "${JWT_SECRETE}",
        "JWT_TOKEN_EXPIRE_AFTER": "1d"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
